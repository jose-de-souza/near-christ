{
  "require": {
    "slim/slim": "^4.0",
    "slim/psr7": "^1.4",
    "illuminate/database": "^8.0",
    "firebase/php-jwt": "^5.2",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
